<Window x:Class="Prueba_de_DI_WFP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prueba_de_DI_WFP"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>

    </Window.Resources>
    <Grid>
        <Button Content="{Binding mensaje}" HorizontalAlignment="Left" Height="44" Margin="46,71,0,0" VerticalAlignment="Top" Width="154" Command="{Binding ConsolaWrite}"/>
        <Label Content="TwoWay DataBinding" HorizontalAlignment="Left" Margin="46,20,0,0" VerticalAlignment="Top"/>
        <Label Content="{Binding json}" HorizontalAlignment="Left" Margin="24,232,0,0" VerticalAlignment="Top" Height="181" Width="301"/>
        <TextBox HorizontalAlignment="Left" Margin="46,161,0,0" Text="{Binding text, UpdateSourceTrigger=PropertyChanged}" TextWrapping="Wrap" VerticalAlignment="Top" Width="679"/>
        <!--<Label HorizontalAlignment="Left" Margin="233,97,0,0" VerticalAlignment="Top" Content="{Binding mensaje}"/>-->

    </Grid>
</Window>
